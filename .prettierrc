{
    "trailingComma": "none",
    "arrowParens": "avoid",
    "singleQuote": true,
    "printWidth": 80,
    "useTabs": false,
    "tabWidth": 4,
    "semi": true,
    "importOrder": [
        "^(next/(.*)$)|^(next$)",
        "<THIRD_PARTY_MODULES>",
        "next-seo.config",
        "^components/(.*)$",
        "^utils/(.*)$",
        "^assets/(.*)$",
        "^@fontsource/(.*)$",
        "^[./]"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true
}
